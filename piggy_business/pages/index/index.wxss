/* ----------------------------团长展示 */
.commander-content-wrap{
  padding: 0 30rpx;
  margin-top: 20rpx;
  overflow: hidden;
  background: #ffffff;
}
.commander-card{
	/* height: 250rpx; */
  margin: 28rpx 0 40rpx;
  padding: 0 20rpx;
	background-color: #ffffff;
	box-shadow: 0px 0px 17px 1px 
		rgba(216, 216, 216, 0.68);
	border-radius: 20rpx;
  overflow: hidden;
}
.commander-card-top{
  margin:40rpx 0 48rpx;
  overflow: hidden;
}
.commander-card-icon{
  width: 96rpx;
	height: 96rpx;
  float: left;
	background-color: #cce198;
  border-radius: 50%;
}
.commander-card-address{
  float: left;
  width: 340rpx;
  margin-left: 22rpx;
}
.commander-card-address text{
  display: block;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.commander-card-address text:nth-of-type(1){
  margin-bottom: 22rpx;
  font-size: 32rpx;
	color: #202020;
}
.commander-card-address text:nth-of-type(2)
{
  font-size: 24rpx;
	color: #999999;
}
.commander-card-opeartion{
  float: right;
}
.commander-card-item{
  float: left;
  width: 72rpx;
	text-align: center;
}
.commander-card-item:nth-child(2){
  margin-left: 22rpx;
}
.commander-card-item image{
  display: block;
  width: 46rpx;
	height: 46rpx;
  /* background: #000; */
  border-radius: 50%;
  margin:0 auto 12rpx;
}

.commander-card-item text{
  display: block;
  font-size: 18rpx;
	color: #666666;
}

.commander-card-right{
  float: right;
  width: 75%;
  font-size: 28rpx;
}
.commander-card-right .btn-logins{
  margin-top: 10rpx;
  float: right;
  color: #333;
  background-color: #fff;
  border:1px solid #666;
}

.buying-preson-wrap{
  float: right;
  margin-bottom: 40rpx;
}
.buying-preson-box{
  float: left;
  display: flex;
  align-items: center;
  margin-right: 10rpx;
}
.buying-preson-box image{
  width: 34rpx;
	height: 34rpx;
  margin-right: 8rpx;
  border-radius: 50%;
	background-color: #cce198;
}
.buying-preson-box text{
  width: 62rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 22rpx;
  line-height: 34rpx;
	color: #666666;
}
.lookmore{
  float: right;
  line-height: 34rpx;
  font-size: 22rpx;
	color: #666666;
}
.lookmore icon{
  float: right;
}
.no-data {
  text-align:center;
  font-size:24rpx;
  color:#999;
  padding-bottom:10rpx;
  padding-top:10rpx;
}
