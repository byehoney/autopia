.productTitle {
  height: auto;
}

.new_windows {
  position: fixed;
  bottom: 155rpx;
  left: 0;
  z-index: 901;
  width: 710rpx;
  /* height: 975rpx; */
}

.new_windows .message {
  width: 100%;
  display: flex;
  align-items: flex-start;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 35rpx;
}

.new_windows .message .headimg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #ebebeb;
}

.new_windows .message .content {
  margin-left: 20rpx;
}

.new_windows .message .content .name {
  font-size: 30rpx;
  color: #fff;
}

.new_windows .message .content .text {
  max-width: 470rpx;
  font-size: 24rpx;
  color: #333;
  padding: 15rpx 24rpx;
  background: #fff;
  border-radius: 0px 35rpx 35rpx 35rpx;
  box-sizing: border-box;
}

.windows_info {
  position: absolute;
  top:56%;
  left:53%;
  transform: translate(-50%,-50%);
  width: 662rpx;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.windows_info .title {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 460rpx;
}

.windows_info .title .item .txt {
  font-weight: 500;
}

.windows_info .title .item .num {
  display: flex;
  align-items: center;
  font-size: 60rpx;
  color: #333;
}

.windows_info .title .item .num .dec {
  font-size: 20rpx;
}

.windows_info .pronum {
  font-size: 24rpx;
  color: #333;
}
.windows_info .pronum text{
  color: #eb3349;
}
.windows_info progress{
  width: 400rpx;
}
button{
  margin: 0;
  padding: 0;
}
button::after{
  border: none;
}
.windows_info button{
  width: 406rpx;
	height: 60rpx;
	border-radius: 30rpx;
  font-size: 28rpx;
	line-height: 60rpx;
	color: #ffffff;
}
.windows_info .shopcontent{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.windows_info .shopcontent .content{
  width: 380rpx;
  height: 150rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.windows_info .shopcontent .content .title{
  width: 360rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
}


.windows_info .shopcontent .shopimg{
  width: 168rpx;
  height: 168rpx;
  background: gainsboro;
  margin-left: 20rpx;
}


.windows_info .shopcontent .content .price{
  width: max-content;
  font-size: 30rpx;
  color: #f62049;
  position: relative;
}
.windows_info .shopcontent .content .price .line{
  position: absolute;
  top:50%;
  width: 100%;
  height: 2px;
  background: #f62049;
}
.windows_info .shopcontent .content .nowprice{
  color: #f62049;
  display: flex;
  align-items: center;
}
.windows_info .shopcontent .content .nowprice .num{
  font-size: 36rpx;
}
.windows_info .shopcontent .content  .nowprice text{
  font-size: 30rpx;
}
.windows_info .shopcontent .content  .nowprice .you{
  width: 120rpx;
	height: 30rpx;
	background-color: #f62049;
	border-radius: 15rpx 15rpx 15rpx 0px;
  font-size: 18rpx;
  color:white;
  line-height: 30rpx;
  text-align: center;
  margin-left:10rpx; 
}
