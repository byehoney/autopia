/* pages/seckill/index.wxss */
.fr{float: right;}
.fl{float: left;}

.seckill .b_banner{height: 750rpx;width: 100%;position: relative;}
.seckill .b_banner image{height: 100%}
.seckill .b_banner span{position: absolute;left: 0;bottom: 50rpx;background-color: #fe6b31;color: #fff;padding: 0 10rpx;font-size: 20rpx;border-radius: 0 5rpx 5rpx 0;height: 38rpx;line-height: 38rpx}
.seckill {background: #fff;}
.seckill .seckillStatus {height: 104rpx;background-color: #2aab54}
.seckill .seckillStatus .statusRow{background-color: #f7ea53;height: 100%;min-width: 80rpx;padding:0 8rpx;
position: relative;}
.seckill .seckillStatus .statusRow:before{content: " ";position: absolute;left: -24rpx;top: 0;bottom: 0;color: #E5000D;border-top: 52rpx solid transparent;border-right: 26rpx solid #f7ea53;border-bottom: 52rpx solid transparent;border-left: 0rpx solid transparent;}
.seckill .seckillStatus .statusRow .h2,.seckill .seckillStatus .statusRow .h3{font-size: 24rpx;color: #2aab54;display: block;text-align: center;}
.seckill .seckillStatus .statusRow .h2{margin-top: 14rpx}
.seckill .seckillStatus .statusRow .h3{font-size: 28rpx;margin-top: 10rpx}
.seckill .seckillStatus .statusRow .h4{font-size: 28rpx;line-height: 104rpx;color: #ff3867;display: block;text-align: center;}
.seckill .seckillStatus .statusRow .red{color: #ff3867}
.seckill .seckillStatus .seckillInfo .price{color: #fff;font-size: 60rpx;vertical-align: middle;line-height: 104rpx;margin:0 10rpx}
.seckill .seckillStatus .seckillInfo .price em{font-size: 46rpx;line-height: 1}
.seckill .seckillStatus .seckillInfo .o_price{vertical-align: middle;display: inline-block;}
.seckill .seckillStatus .seckillInfo .o_price p{font-size: 26rpx;color: #fff;float: left;clear: both;text-decoration: line-through;}
.seckill .seckillStatus .seckillInfo .o_price em{font-size: 24rpx;color: #fff;padding: 5rpx 10rpx;background-color: rgba(0,0,0,.1);border-radius: 10rpx;float: left;clear: both;margin-top: 10rpx}
.seckill .seckillStatus.on{background-color: #ff3867}
.seckill .seckillStatus.gray{background-color: #bab9b9}
.seckill .seckillStatus.gray .statusRow{background-color: #a6a6a6}
.seckill .seckillStatus.gray .statusRow .h2,.seckill .seckillStatus.gray .statusRow .h3,.seckill .seckillStatus.gray .statusRow .h4{color: #fff}
.seckill .seckillStatus.gray .statusRow:before{border-right-color: #a6a6a6}
.seckill .seckillStatus .statusRow.gray{background-color: #a6a6a6}
.seckill .seckillStatus .statusRow.gray .h2,.seckill .seckillStatus .statusRow.gray .h3,.seckill .seckillStatus .statusRow.gray .h4{color: #fff}
.seckill .seckillStatus .statusRow.gray:before{border-right-color: #a6a6a6}
.seckill .seckillStatus .statusRow.gray .countDown em {color: #fff;}
.seckill .seckillStatus .statusRow.gray .countDown span {background-color: #fff;color: #666;}
.countDown .wrap{padding: 30rpx 24rpx;padding-top: 0}
.countDown span,.countDown em{display: inline-block;vertical-align: middle;}
.countDown span{min-width: 45rpx;height: 45rpx;border-radius: 10rpx;line-height: 45rpx;text-align: center;background-color: #ff3867;font-size: 30rpx;color: #fff}
.countDown em{font-size: 26rpx;color: #ff3867;margin: 0 10rpx}
.countDown em.text{font-size: 28rpx}

.seckill .shareBlock{height: 140rpx}
.shareBtn::after,.shareBtn::before {display: none;}
.seckill .shareBlock .shareBtn{padding: 0;margin: 0;height: 100%;width:150rpx;background-color: #ff3867;text-align: center;margin-left: 10rpx;line-height: 1;border-radius: 0}
.seckill .shareBlock .shareBtn:before{display: none;}
.seckill .shareBlock .shareBtn i{font-size: 50rpx;color: #fff;position: relative;top: 24rpx}
.seckill .shareBlock .shareBtn p{font-size: 24rpx;color: #fff;display: block;margin-top: 40rpx}
.seckill .shareBlock .seckillName{height: 100%}
.seckill .shareBlock .seckillName .table{display: flex;height: 100%;padding-left: 20rpx;overflow: hidden;align-items: center}
.seckill .shareBlock .seckillName .t_cell{vertical-align: middle;font-size: 34rpx;color: #222;-webkit-line-clamp: 3;}
.seckill .shareBlock .seckillName .t_cell em{color: #888;font-size: 28rpx;margin-left: 10rpx}
.seckill .share_p{background:#fff;}
.seckill .share_p .wrap{background:#FFCEDB;font-size: 26rpx;color: #ff3867;text-align: center;padding: 20rpx 0;}

.seckill .team {padding-bottom:20rpx;}
.seckill .team .hd{padding: 15rpx 20rpx}
.seckill .team .hd i{font-size: 40rpx;color: #ff3867;margin-right: 10rpx}
.seckill .team .hd span{font-size: 30rpx;color: #666}
.seckill .team .bd .li{text-align: center;padding: 30rpx 0;margin: 0 14rpx;}
.seckill .team .bd .li image{width: 100rpx;height: 100rpx;border-radius: 100%}
.seckill .team .ft{text-align: center;}
.seckill .team .ft span{display: inline-block;padding: 10rpx 20rpx;font-size: 30rpx;color: #ff3867;border-radius: 20rpx;border:1px solid #ff3867;line-height: 1}

.seckill .d_desc {margin-bottom: 100rpx;}
.seckill .d_desc .secTit{padding: 0 20rpx;border-top:20rpx solid #eff4f4;}
.seckill .d_desc .secTit .h3{font-size: 32rpx;color: #ff3867;text-align: center;line-height: 95rpx;}

.seckill .d_desc .pureText .wrap{padding:  20rpx;padding-top:0;}
.seckill .d_desc .pureText{font-size: 26rpx;color: #222;}
.seckill .d_desc .thisDesc{font-size: 24rpx;padding: 12rpx;padding-bottom: 30rpx;}
.seckill .d_desc .thisDesc image:nth-of-type(1){
  margin-top: 0;
}
.seckill .d_desc .thisDesc image{max-width: 100%;margin-top: -8rpx;}
.seckill .d_desc .thisDesc p{display: block;}



.seckill .ftBtn{height: 100rpx;text-align: center;line-height: 100rpx;color: #fff;position: fixed;bottom: 0;left: 0;right: 0;z-index: 5;border-radius:0;}
.seckill .ftBtn.redBtn{background-color: #ff3867}
.seckill .ftBtn.grayBtn{background-color: #a6a6a6}

.seckill{padding-bottom: 20rpx}
.showTxt image {display:inline-block;width:400rpx;height:400rpx;}

.mask{z-index:20;position:fixed;top:0;right:0;bottom:0;left:0;width:100%;background-color:#000;opacity:.7;filter:alpha(opacity=80);-moz-opacity:.8;-khtml-opacity:.8;}
.seckill .teamList  {position: fixed;bottom: 0;left: 0;right: 0;z-index: 30;background-color: #F3F3F3;overflow: hidden;border-radius:20rpx 20rpx 0 0;}
.seckill .teamList  .h2{font-size: 34rpx;color: #222;text-align: center;display: block;height: 90rpx;line-height: 90rpx}
.seckill .teamList  .wrap{padding: 0 30rpx;background: #fff;}
.seckill .teamList  span.iconfont{position:absolute;right:35rpx;font-size:36rpx;top:30rpx;}
.seckill .teamList  .t_list{height: 670rpx;overflow: auto;}
.seckill .teamList  .t_list .li{padding: 20rpx 0;}
.seckill .teamList  .t_list .li:last-child{border-bottom: 0}
.seckill .teamList  .t_list image{width: 100rpx;height: 100rpx;border-radius: 100%;margin-right: 20rpx}
.seckill .teamList  .t_list .name{font-size: 30rpx;color: #333;display: block;padding-top: 12rpx}
.seckill .teamList  .t_list .time{font-size: 24rpx;color: #999;display: block;line-height: 2}
.seckill .teamList  .t_list span.cut{color:#222;font-size: 30rpx;line-height: 100rpx}
.seckill .teamList  .t_list span.cut em{color: #f9295a;font-size: 30rpx}
.seckill .teamList  .t_list .desc{overflow: hidden;border-bottom: 1px solid #dfdfdf;height: 100rpx}


