}

.navbar{
  position: relative;
}
.back-box{
position:absolute;
width:100%;
left:0rpx;
top:64rpx;
display:flex;
align-items:center;
box-sizing: border-box;
padding-left: 20rpx;
padding-right: 45rpx;
}
.title{
  flex: 1;
  text-align: center;
  font-size: 28rpx;
}
.img_bg {
  width: 100vw;
  height: 560rpx;
}

.vip-head-con {
  color: #f9e4ad;
  text-align: center;
}
.vip-head-con .vip-title {
  color: #f9e4ad;
  font-weight: bold;
  font-size: 40rpx;
  text-align: center;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
  letter-spacing: 8rpx;
}

.vip-head-con .vip-title+view {
  font-size: 28rpx;
  color: #a49985;
}
.title-desc{
  position: relative;
  padding:0 20rpx;
}
.title-desc::before{
  content: '';
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 8rpx;
	height: 14rpx;
	border-top: solid 3px #fae3af;
  border-left: solid 3px #fae3af;
}
.title-desc::after{
  content: '';
  position: absolute;
  bottom: 0rpx;
  right: 6rpx;
  width: 8rpx;
	height: 14rpx;
	border-bottom: solid 3px #fae3af;
  border-right: solid 3px #fae3af;
}
.vip-head-con .vip-title+view>text {
  color: #595143;
}

.vip-main {
  width: 100%;
  background: #fff;
  position: relative;
  box-sizing: border-box;
}

.vip-main .main-top {
  width: 94%;
  min-height: 466rpx;
  border-radius: 30rpx;
  box-shadow: 0 0 30rpx 5rpx rgba(0, 0, 0, 0.3);
  position: absolute;
  left: 3%;
  top: -84rpx;
  overflow: hidden;
}

.main-top-title {
  width: 100%;
  height: 100rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.main-top-title>view:nth-child(1) {
  font-size: 28rpx;
  color: #353535;
}

.main-top-title>view:nth-child(2) {
  font-size: 24rpx;
  color: #ccc;
}

.main-process-tip {
  width: 100%;
  text-align: center;
  color: #353535;
  font-size: 28rpx;
  font-weight: bold;
  height: 84rpx;
  line-height: 84rpx;
  background: #fff;
}

/* 进度条 */

.process-bar-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 90rpx;
}

.process-bar {
  width: 100%;
  height: 10rpx;
  background: #e7e7e7;
  position: relative;
  border-radius: 5rpx;
}
.process-bar-box.distance{
  margin-bottom: 40rpx;
}

.process-bar .process-real-box {
  width: 50%;
  height: 15rpx;
  border-radius: 8rpx;
  background: #cca973;
  position: absolute;
  left: 0;
  top: -2.5rpx;
}

.bar-bottom {
  width: 100%;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
  height: 77rpx;
  background: #fff;
}

.bar-bottom>text {
  font-size: 28rpx;
  color: #999;
}

/* 立即邀人 */

.btn-inviter {
  width: 84%;
  height: 132rpx;
  margin: 0 auto;
  position: relative;
}

.btn-inviter>image {
  width: 100%;
  height: 100%;
}

.btn-inviter .inviter-con {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
  display: flex;
  color: #fff;
  padding-left: 154rpx;
  padding-right: 25rpx;
  /* justify-content: center; */
  align-items: center;
}

.inviter-txt {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  font-style: italic;
  letter-spacing: 8rpx;
}

.circel {
  width: 90rpx;
  height: 90rpx;
  background: #efe3d7;
  border-radius: 50%;
  display: flex;
  align-items: center;
  animation: viewlinear 2s linear infinite;
}

/* .circel>image{
  width: 100%;
} */

@keyframes viewlinear {
  /** 第一种写法**/

  0% {
    width: 90rpx;
    height: 90rpx;
  }

  50% {
    width: 110rpx;
    height: 110rpx;
  }

  100% {
    width: 90rpx;
    height: 90rpx;
  }
}

.circel>text {
  font-size: 24rpx;
  color: #353535;
}

.circel>text:nth-child(1) {
  flex-wrap: wrap;
  flex: 1;
  text-align: right;
}

.circel>text:nth-child(2) {
  width: 32rpx;
  text-align: left;
  font-size: 24rpx;
  margin-top: 2rpx;
}

/* 会员权益 */

.vip-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 25rpx;
  margin-bottom: 150rpx;
}

.vip-box .vip-title {
  width: 342rpx;
  height: 54rpx;
  margin: 0 auto 45rpx;
