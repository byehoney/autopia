   min-height: 300rpx;
}

.vip-info .vip-info-con>view {
  margin-bottom: 6rpx;
}

/* 会员专享 */

.vip {
  width: 100%;
  box-sizing: border-box;
  padding: 0 25rpx;
  margin-top: 75rpx;
}

