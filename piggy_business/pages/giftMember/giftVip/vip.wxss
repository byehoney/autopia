  font-size: 28rpx;
  /* text-indent:56rpx; */
  box-shadow: 0 5rpx #a4763e;
 
}

.vip-info .vip-info-con {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  border-radius: 20rpx;
