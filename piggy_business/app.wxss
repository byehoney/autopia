.register_box .agreement {
  width: 80%;
  height: 50rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 0rpx auto;
}

.register_box .agreement .radio {
  margin-right: 20rpx;
}

.register_box .agreement .radio checkbox {
  transform: scale(0.8);
}

.register_box .agreement  .text text {
  color: blue;
}

.agreement_box {
  width: 90%;
  position: fixed;
  z-index: 1000;
  top: 5%;
  left: 5%;
  height: 90%;
  background: #fff;
  /* padding-bottom: 48px; */
  box-sizing: border-box;
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
  flex-flow: column;
}

.agreement_box .head {
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
}

.agreement_box .head .left {
  position: absolute;
  left: 10rpx;
}

.agreement_box .body {
  flex: 1;
  width: 100%;
  overflow: hidden;
  overflow-y: auto;
  /* min-height: 50%; */
}

.agreement_box .body .com {
  width: 100%;
  height: auto;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}

.agreement_box  .foot {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
}
