/*9.16 引导页模式切换样式*/

.register_box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  background: rgba(0, 0, 0, 0.4);
  overflow: hidden;
  z-index: 999;
}

.register_box .container {
  width: 70%;
  background: #fff;
  margin: 20% auto;
  border-radius: 20rpx;
  padding: 0;
  min-height: auto;
}

.register_box .container .tit {
  font-size: 20px;
  line-height: 60rpx;
  text-align: center;
  font-weight: bold;
  padding: 30rpx 0 10rpx 0;
}

.register_box .container .item {
  width: 80%;
  border: 1rpx solid #999;
  height: 70rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 40rpx auto;
  border-radius: 10rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.register_box .container .item2 {
  width: 80%;
  height: 70rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 40rpx auto;
}

.register_box .container .item input {
  width: 100%;
  height: 100%;
}

.register_box .container .item2 .inp {
  width: 50%;
  height: 100%;
  border: 1rpx solid #999;
  border-radius: 10rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  margin-right: 10%;
  line-height: 80rpx;
}

.register_box .container .item2 .inp input {
  height: 100%;
  width: 100%;
}

.register_box .container .item2 .getCode {
  width: 40%;
  text-align: center;
  height: 100%;
  border-radius: 10rpx;
  padding: 0 10rpx;
  line-height: 70rpx;
  box-sizing: border-box;
  border: 1rpx solid #8fbeec;
  color: #8fbeec;
}

.btnbox {
  width: 80%;
  height: 80rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 50rpx auto;
  margin-top: 20rpx;
}

.btnbox .but {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 20rpx;
  width: 180rpx;
  text-align: center;
  /* border: 1rpx solid #8fbeec;
  border-radius: 5rpx; */
}

.btnbox  .login {
  background: #8fbeec;
  color: #fff;
}

.login_bg {
  background: #8fbeec;
  color: #fff;
}

.login_bg2 {
  background: #b4b4b4;
  color: #fff;
}

button[disabled].login_bg2 {
  background: #b4b4b4 !important;
  color: #fff !important;
}

