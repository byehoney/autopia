/* pages/withDrawSucc/withDrawSucc.wxss */
page{
  background-color: #F8F8F8;
}
.main{
  margin-top: 30rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  flex-shrink: 1;
  height: calc(100vh - 30rpx);
  text-align: center;
  align-items: center;
  position: relative;
}
.icon{
  width: 178rpx;
  height: 190rpx;
  margin-top: 190rpx;
}
.tip{
  font-size:42rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:59rpx;
  margin-top: 40rpx;
}
.back{
  position: absolute;
  bottom: 228rpx;
  left: 50%;
  margin-left: -275rpx;
  width:550rpx;
  height:98rpx;
  border-radius:66rpx;
  border:1px solid rgba(244,58,65,1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(244,58,65,1);
  line-height:50rpx;
}
.inviteBtn{
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  margin-left: -275rpx;
  width:550rpx;
  height:98rpx;
  background:linear-gradient(90deg,rgba(250,108,118,1) 0%,rgba(244,58,65,1) 100%);
  box-shadow:0px 10rpx 20rpx 0px rgba(203,70,75,0.3);
  border-radius:66rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:50rpx;
}