} */
.cardList .cardItem .gift{
  width: 245rpx;
  height: 306rpx;
  padding: 7rpx 6rpx 13rpx;
  background-color: #fff;
  border-radius: 6rpx;
}
.cardList .cardItem .gift .goodsImg{
  width:243rpx;
  height: 194rpx;
  display: block;
  margin: 0 auto 12rpx;
}
.cardList .cardItem .gift .goodsName{
  font-size:23rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,116,79,1);
  line-height:28rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.cardList .cardItem .gift .goodsInfo{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-top: 4rpx;
}
.cardList .cardItem .gift .price{
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(96,65,34,1);
  line-height:40rpx;
}
.cardList .cardItem .gift .vipIcon{
  width: 56rpx;
  height: 30rpx;
}

.act_area{
  width: 702rpx;
  margin: 0 auto 28rpx;
}
.act_area .act_head{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 15rpx;
}
.act_head .headLeft,.act_head .headRight{
  display: flex;
  align-items: center;
}
.act_head .headLeft .act_icon{
  width: 34rpx;
  height: 34rpx;
  flex-shrink: 0;
  margin-right: 11rpx;
}
.act_head .headLeft .act_name{
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:50rpx;
}
.act_head .headRight .act_name{
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(124,124,124,1);
  line-height:37rpx;
}
.act_head .headRight .act_icon{
  width: 11rpx;
  height: 20rpx;
  margin-left: 8rpx;
}
.act_body,.actList{
  height: 340rpx;
}
.actList .gift{
  width: 195rpx;
  height: 340rpx;
  background-color: #F6F7F9;
  border-radius: 6rpx;
}
.actList .gift .goodsImg{
  width: 195rpx;
  height: 195rpx;
}
.actList .gift .goodsName{
  padding: 0 12rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:32rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 11rpx;
}
.actList .gift .goodsInfo{
  padding: 0 12rpx;
  display: flex;
  align-items: center;
}
.actList .gift .price{
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,59,48,1);
  line-height:40rpx;
  margin-right: 16rpx;
}
.actList .gift .actIcon{
  width: 32rpx;
  height: 32rpx;
}
.goodsArea{
  width: 702rpx;
  margin: 0 auto 60rpx;
}
.goodsArea .goodsHeader{
  display: flex;
  align-items: center;
}
.goodsArea .goodsHeader .headerIcon{
  width: 32rpx;
  height: 32rpx;
  margin-right: 14rpx;
}
.goodsArea .goodsHeader .title{
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:50rpx;
}
.goodsList .goodsItem{
  display: flex;
  align-items: center;
  justify-content: space-between;
  min-height: 240rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #EEEEEE;
}
.goodsList .goodsItem:last-child{
  border-bottom: none;
}
.goodsItem .goodsLeft .goodsImg{
  width:240rpx;
  height:240rpx;
  border-radius:6rpx;
}
.goodsItem .goodsRight{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  min-height: 240rpx;
  margin-left: 30rpx;
}
.goodsItem .goodsRight .goodsName{
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:45rpx;
   text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goodsItem .goodsRight .goodsProm{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:33rpx;
  margin-top: 10rpx;
}
.goodsRight .goodsProm .promItem{
  position: relative;
}
.goodsRight .goodsProm .promItem::after{
  content: '';
  display: inline-block;
  width: 2rpx;
  height: 24rpx;
  background-color: rgba(153,153,153,1);
  position: relative;
  top: 2rpx;
  margin: 0 10rpx;
}
.goodsRight .goodsProm .promItem:last-child::after{
  content: '';
  display: none;
}
.goodsRight .goodsIcons{
  display: flex;
  align-items: center;
  margin-top: 15rpx;
}
.goodsRight .goodsIcons .vipIcon{
  width:65rpx;
  height:30rpx;
  background:rgba(227,196,140,1);
  border-radius:2rpx;
  font-size:20rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:30rpx;
  text-align: center;
}
.goodsRight .goodsIcons  .goodsBack{
  display: flex;
  align-items: center;
  margin-left: 14rpx;
}
.goodsIcons  .goodsBack .goodsTip{
  width:48rpx;
  height:30rpx;
  background:linear-gradient(90deg,rgba(254,116,123,1) 0%,rgba(244,60,67,1) 100%);
  border-radius:2rpx 0px 0px 2rpx;
  font-size:20rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:30rpx;
  text-align: center;
}
.goodsIcons  .goodsBack .backNum{
  width:62rpx;
  height:26rpx;
  font-size:20rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(244,61,68,1);
  line-height:26rpx;
  padding: 0 5rpx;
  border:1rpx solid #F43D44;
  border-radius: 2rpx;
}
.goodsRight .goodsInfo{
  display: flex;
  align-items: center;
  margin-top: 7rpx;
  justify-content: space-between;
}
.goodsRight .goodsInfo .price{
  display: flex;
  align-items: flex-end;
}
.goodsRight .goodsInfo .goodsPrice{
  font-size:38rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(244,60,67,1);
  line-height:53rpx;
}
.goodsRight .goodsInfo .mPrice{
  font-size:24rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(150,150,150,1);
  line-height:33rpx;
  text-decoration: line-through;
  margin-left: 15rpx;
  position: relative;
  top: -5rpx;
}
.goodsRight .goodsInfo .goodsBtn{
  width:154rpx;
  height:55rpx;
  border-radius:28rpx;
  border:1px solid rgba(244,56,63,1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:30rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,59,48,1);
}
.masklog{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.5);
  z-index: 12;

}
.masklog .modallog{
  width: 750rpx;
  height: 1020rpx;
  background: url('https://ctb-public-file.oss-cn-beijing.aliyuncs.com/shop/indexLog.png') no-repeat 0 0 ;
  background-size: 100% 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -442rpx;
  margin-left: -380rpx;

}
.modallog .close{
  position: absolute;
  top: -31rpx;
  right: 20rpx;
  width: 38rpx;
  height: 38rpx;
  z-index: 101;
}

.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.5);
  z-index: 10;
  display: none;
  
}
.mask .modal{
  width: 530rpx;
  height: 884rpx;
  background: url('https://ctb-public-file.oss-cn-beijing.aliyuncs.com/shop/tanchuang.png') no-repeat 0 0 ;
  background-size: 100% 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -442rpx;
  margin-left: -265rpx;
}
.modal .close{
  position: absolute;
  top: 0rpx;
  right: 0;
  width: 38rpx;
  height: 38rpx;
  z-index: 100;
}
.modal .inner{
  width: 358rpx;
  height: 324rpx;
  position: absolute;
  top: 330rpx;
  left: 35rpx;
  background-color: transparent;
  padding: 38rpx 50rpx;
}
.modal .priceItem{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(87,87,87,1);
  line-height:33rpx;
}
.modal .priceItem:first-child{
  margin-bottom: 29rpx;
}
.modal .priceItem:nth-child(2){
  margin-bottom: 22rpx;
}
.modal .priceItem:nth-child(3),.modal .priceItem:nth-child(6){
  font-size:30rpx;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:rgba(194,141,70,1);
  line-height:42rpx;
  margin-bottom: 33rpx;
}
.modal .priceItem:nth-child(3) .priceName,.modal .priceItem:nth-child(6) .priceName{
  position: relative;
  left: -28rpx;
}
.modal .divide{
  width:370rpx;
  border-bottom:2rpx dashed rgb(0, 0, 0,0.2);
  margin: 0 auto 34rpx;
}
.modal .priceItem:nth-child(5){
  margin-bottom: 23rpx;
}
.modal .btn{
  width:457rpx;
  height:64rpx;
  background:linear-gradient(311deg,rgba(219,164,92,1) 0%,rgba(253,230,197,1) 100%);
  box-shadow:0rpx 7rpx 11rpx 0px rgba(218,178,120,0.35);
  border-radius:40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:30rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(103,71,47,1);
  position: absolute;
  bottom: 55rpx;
  left: 37rpx;
}
.foodItem{
  height: 300rpx;
  display: flex;
  align-items: center;
  z-index: 1001
}

.goodsItemRight{
  display: flex;
  flex-direction: column;
  width: 430rpx;
  height: 250rpx;
  /* background: lightskyblue; */
  justify-content: space-between;
}

.goodTitle text{
  height:auto;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;

}
.goodLabel text{
  height:auto;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;

}
