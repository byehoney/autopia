/**index.wxss**/
.header{
  width: 100%;
  background-color: #fff;
  display: flex;
  justify-content: center;
}
.header .inner{
  width: 686rpx;
  height: 70rpx;
  border-radius:35rpx;
  display: flex;
  align-items: center;
  background-color: #F0F2F5;
}
.inner .icon{
  width: 26rpx;
  height: 26rpx;
  margin-left: 27rpx;
}
.inner .text{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:40rpx;
  margin-left: 8rpx;
}
.homeBanner{
  width: 700rpx;
  height: 350rpx;
  margin: 30rpx auto 38rpx;
}
.homeBanner .bannerItem , .homeBanner .slideImg{
  width: 100%;
  height: 350rpx;
}
.typeList{
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  flex-wrap: wrap;
  margin-bottom: 6rpx;
}
.typeList .typeItem{
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-bottom: 24rpx;

}
.typeList .typeItem .typeIcon{
  width: 96rpx;
  height: 96rpx;
}
.typeList .typeItem .typeText{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:32rpx;
  text-align: center;
}
.adBanner{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
}
.adBanner .adImg{
  width: 695rpx;
  /* height: 174rpx; */
}
.rightArea{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 695rpx;
  height: 345rpx;
  margin: 0 auto 30rpx; 
}
.rightArea .left{
  width:345rpx;
  height:345rpx;
  background:rgba(216,216,216,1);
  border-radius:8rpx;
}
.rightArea .right{
  width:342rpx;
  height:345rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.rightArea .right .top,.rightArea .right .bottom{
  width:342rpx;
  height:165rpx;
  background:rgba(216,216,216,1);
  border-radius:8rpx;
}
.rightArea .left .lfImg{
  width: 345rpx;
  height: 345rpx;
}
.rightArea .right .top .rgImg,.rightArea .right .bottom .rgImg{
  width: 342rpx;
  height: 165rpx;
}
.cardArea{
  width: 702rpx;
  height: 440rpx;
  background: url('https://ctb-public-file.oss-cn-beijing.aliyuncs.com/shop/heikaliwu.png') no-repeat 0 0;
  background-size: 100% 100%;
  margin: 0 auto 33rpx;
}
.cardVip{
  width: 702rpx;
  margin: 0 auto 33rpx;
}
.cardVip image{
  width: 100%;
}
.cardList{
  width: 100%;
  height: 326rpx;
  position: relative;
  top: 92rpx;
}
