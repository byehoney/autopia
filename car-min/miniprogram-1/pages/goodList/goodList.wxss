/* pages/goodList/goodList.wxss */
.head{
  display: flex;
  flex-direction:row;
  height: 110rpx;
  position: fixed;
  z-index: 999;
  width: 750rpx;
  background: white;
}
.header-item {
  display: flex;
  width: 150rpx;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  background-color: white;
  z-index: 999;
  height: 110rpx;
  position: relative;
  
}
.foodItem{
  height: 300rpx;
  display: flex;
  align-items: center;
  z-index: 1001
}
.goodsItemRight{
  display: flex;
  flex-direction: column;
  width: 430rpx;
  height: 250rpx;
  /* background: lightskyblue; */
  justify-content: space-between;
}

.goodTitle text{
  height:auto;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;

}
.goodLabel text{
  height:auto;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;

}

.contacButton {
  /* 设置背景无色 */
  background-color: transparent;
  /* padding: 0;
  margin: 0; */
  font-size: inherit;
  /* height: 300rpx; */
  z-index: 999;
  width: 75rpx;
  display: flex;
  margin-left: -75rpx;

}
 
.contacButton::after {
  border: #FFF none;
}